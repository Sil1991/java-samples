<?xml version="1.0" encoding="ISO-8859-1" ?>
<!DOCTYPE struts-config PUBLIC
          "-//Apache Software Foundation//DTD Struts Configuration 1.3//EN"
          "http://struts.apache.org/dtds/struts-config_1_3.dtd">
<struts-config>

	<action-mappings>
		<action path="/HelloWorld" type="net.lshimokawa.ejemplos.struts.HelloWorldAction">
			<forward name="helloWorld" path="/WEB-INF/jsp/helloWorld.jsp"></forward>
		</action>

		<action path="/HelloWorldDispatch"
			type="net.lshimokawa.ejemplos.struts.HelloWorldDispatchAction"
			parameter="accion">
			<forward name="helloWorld" path="/WEB-INF/jsp/helloWorldDispatch.jsp"></forward>
		</action>
	</action-mappings>

</struts-config>